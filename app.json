{
  "expo": {
    "name": "fit_wear_app",
    "slug": "fit_wear_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-new.png",
    "scheme": "fitwearapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitwear.app",
      "icon": "./assets/images/icon-new.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-new.png",
        "backgroundColor": "#1B3B5D"
      },
      "package": "com.fitwear.app",
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-new.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1B3B5D",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5edc6471-15cf-4bac-b9f8-6a83f5c4d036"
      }
    }
  }
}